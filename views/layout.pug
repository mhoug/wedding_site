html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title #{title}
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(href='https://fonts.googleapis.com/css?family=Work+Sans:400,300,600,400italic,700', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Sacramento', rel='stylesheet')
    link(rel='stylesheet', href='/stylesheets/css/animate.css')
    link(rel='stylesheet', href='/stylesheets/css/icomoon.css')
    link(rel='stylesheet', href='/stylesheets/css/bootstrap.css')
    link(rel='stylesheet', href='/stylesheets/css/magnific-popup.css')
    link(rel='stylesheet', href='/stylesheets/css/owl.carousel.min.css')
    link(rel='stylesheet', href='/stylesheets/css/owl.theme.default.min.css')
    link(rel='stylesheet', href='/stylesheets/css/style.css')
    script(src='/javascripts/js/modernizr-2.6.2.min.js')
    script(src='/javascripts/js/map-location.js')
    script(async defer src='https://maps.googleapis.com/maps/api/js?key=AIzaSyD41NhDoSWrADWa0Z8dxdFwYP80-GQxiEM&callback=initialize')
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]><script src="js/respond.min.js"></script><![endif]-->

  body
    .fh5co-loader
    #page
      nav.fh5co-nav(role='navigation')
        .container
          .row
            .col-xs-2
            .col-xs-10.text-right.menu-1
              ul
                li
                  a(href='/') #lovebyrds2017
                li
                  a(href='/') #byrdsofafeather

      block content

      #fh5co-started.fh5co-bg(style='background-image:url(images/img_bg_1.jpg);')
        .overlay
        .container
          .row.animate-box
            .col-md-8.col-md-offset-2.text-center.fh5co-heading
              h2 Gift Registry
              p Your presence is all we wish for on our wedding day! However, for those wishing to contribute in need of gift suggestions...
          .row.animate-box
            .col-md-10.col-md-offset-1
              .col-md-4.col-sm-4.text-center
                a.macys(href='https://www.macys.com/wgl/registry/guest/6790325')
                  img(src='images/macys.jpg' style='width:100%' alt="Macy's")
              .col-md-4.col-sm-4.text-center
                a.target(href='http://tgt.gifts/lovebyrds17')
                  img(src='images/target.jpg' style='width:100%' alt="Target")
              .col-md-4.col-sm-4.text-center
                a.bed(href='https://www.bedbathandbeyond.com:443/store/giftregistry/view_registry_guest.jsp?registryId=544565762&eventType=Wedding&pwsurl')
                  img(src='images/bed.jpg' style='width:100%' alt="Bed Bath & Beyond")
          .row.copyright(style='font-size:14px; color: #fff;')
            .col-md-12.text-center
              p(style='margin-top: 60px;')
                small.block
                  | Designed by Lyndall Moore

    .gototop.js-top
      a.js-gotop(href='#')
        i.icon-arrow-up

    script(src='/javascripts/js/jquery.min.js')
    script(src='/javascripts/js/jquery.easing.1.3.js')
    script(src='/javascripts/js/bootstrap.min.js')
    script(src='/javascripts/js/jquery.waypoints.min.js')
    script(src='/javascripts/js/owl.carousel.min.js')
    script(src='/javascripts/js/jquery.countTo.js')
    script(src='/javascripts/js/jquery.stellar.min.js')
    script(src='/javascripts/js/jquery.magnific-popup.min.js')
    script(src='/javascripts/js/magnific-popup-options.js')
    script(src='/javascripts/js/simplyCountdown.js')
    script(src='/javascripts/js/main.js')
    script.
      var d = new Date(2017, 10, 4, 16, 0, 0, 0);
      // default example
      simplyCountdown('.simply-countdown-one', {
        year: d.getFullYear(),
        month: d.getMonth() + 1,
        day: d.getDate()
      });
      //jQuery example
      $('#simply-countdown-losange').simplyCountdown({
        year: d.getFullYear(),
        month: d.getMonth() + 1,
        day: d.getDate(),
        enableUtc: false
      });
